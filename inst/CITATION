meta <- packageDescription("rCAX")
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)  
bibentry(
bibtype = "TechReport",
title = paste0(meta$Package, ": ", meta$Title, ". ", note),
author = "E. E. Holmes and M. Williams",
year = year, 
institution = "NOAA Fisheries, Northwest Fisheries Science Center, Math Bio Program",
doi = "10.5281/zenodo.7402463")

bibentry(
bibtype = "Misc",
year = "https://www.streamnet.org/home/data-maps/fish-hlis/",
title = "Coordinated Assessments Partnership (CAP) fish high-level indicators (HLIs)",
note = "For details on citing StreamNet content see: https://www.streamnet.org/resources/citing-sn/")

